{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "TodoApi.Services.TokenService": "Information"
    }
  },
  "OpenTelemetry": {
    "ServiceName": "TodoApi",
    "OtlpEndpoint": "",
    "ConsoleExporterEnabled": false
  },
  "Jwt": {
    "Key": "${JWT_SECRET_KEY}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_CONNECTION_STRING}",
    "Redis": "${REDIS_CONNECTION_STRING}"
  }
}
